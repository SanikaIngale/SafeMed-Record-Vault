{
  "expo": {
    "name": "SafeMed-Record-Vault",
    "slug": "SafeMed-Record-Vault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": [
      {
        "asset": "@expo-google-fonts/poppins/Poppins_400Regular.ttf",
        "family": "Poppins_400Regular"
      },
      {
        "asset": "@expo-google-fonts/poppins/Poppins_600SemiBold.ttf",
        "family": "Poppins_600SemiBold"
      },
      {
        "asset": "@expo-google-fonts/poppins/Poppins_700Bold.ttf",
        "family": "Poppins_700Bold"
      }
    ]
  }
}
